<form class="{{cssClass}} flexcol" autocomplete="off">
    {{!-- Sheet Header --}}
  <header class="sheet-header flexrow">
    <img class="profile" src="{{actor.img}}" data-edit="img" title="{{actor.name}}" />

    

    <section class="header-details flexrow">
      <h1 class="charname"><input name="name" type="text" value="{{actor.name}}" placeholder="Name"/></h1>
      <aside class="header-origin flexcol">
        <div class="charorigin">
          <label>Origin</label>
          <span class="origins"></span>
        </div>
          <div class="origin-select flexrow">
          <select name="data.characterOrigins">
          {{#select data.data.characterOrigins }}
          {{#each config.characterOrigins as |name type| }}
          <option value="{{type}}">{{name}}</option>
          {{/each}}
          {{/select}}
        </select>
        </div>
        
      </aside>

      {{!-- Character Summary --}}
        <ul class="summary flexrow">
          <li>
            <input name="data.secretID" type="text" value="{{data.data.secretID}}" placeholder="Secret ID"/>
          </li>
          <li>
            <input type="text" name="data.groupAffliation" type="text" value="{{data.data.groupAffliation}}" placeholder="Group Affliation"/>
          </li>
        </ul>

        {{!-- Header Attributes --}}
        <ul class="attributes flexrow">
          <li class="attribute health">
            <h4 class="attribute-name box-title">Health / Max Health</h4>
            <div class="attribute-value multiple">
          <input type="text" name="data.health.value" value="{{data.data.health.value}}" data-dtype="Number"/>
          <span class="sep"> / </span>
          <input type="text" name="data.health.max" value="{{data.data.health.max}}" data-dtype="Number"/>
          </div>

          </li>

          <li class="attribute karma">
            <h4 class= "attribute-name box-title">Karma</h4>
            <div class="resource-content flexrow flex-center flex-between">
            <input type="text" name="data.karma.value" value="{{data.data.karma.value}}" data-dtype="Number"/>
            </div>
          </li>

          

          <li class="attribute popularity">
            <h4 class="attribute-name box-title">Popularity</h4>
            <div class="attribute-value multiple">
          <input type="text" name="data.popularity.value" value="{{data.data.popularity.value}}" data-dtype="Number"/>
            </div>
          </li>

          <li class="attribute power-talents">
            <h4 class="attribute-name box-title">Powers / Talents</h4>
            <div class="attribute-value multiple">
              <input type="number" />
              <span class="sep"> / </span>
              <input type="number" / >
            </div>
            
          </li>
        </ul>



      {{!-- The grid classes are defined in scss/global/_grid.scss. To use,
      use both the "grid" and "grid-Ncol" class where "N" can be any number
      from 1 to 12 and will create that number of columns.  --}}
      

          

        

        

        
        </section>
        
    
    </header>

    {{!-- Sheet Tab Navigation --}}
  <nav class="sheet-tabs tabs" data-group="primary">
    {{!-- Default tab is specified in actor-sheet.mjs --}}
    <a class="item" data-tab="attributes">Attributes</a>
    <a class="item" data-tab="description">Powers</a>
    <a class="item" data-tab="items">Talents</a>
    <a class="item" data-tab="powers">Contacts</a>
    <a class="item" data-tab="talents">Items</a>
  </nav>

 <section class="sheet-body">

        <div class="tab attributes flexrow" data-group="primary" data-tab="attributes">

            {{!-- Ability Scores --}}
             <ul class="ability-scores flexcol">
            
                <li class="ability" data-ability="{{id}}">
                    <h4 class="ability-name box-title rollable">Fighting</h4>
                    <select name="data.attackTypes">
                      {{#select data.data.initialRankValues }}
                      {{#each config.initialRankValues as |name type| }}
                      <option value="{{type}}">{{name}}</option>
                      {{/each}}
                      {{/select}}
                      </select>
                     <div class="attribute-value multiple">
          <input type="number" name="data.fighting.value" value="{{data.data.fighting.value}}" data-dtype="Number"/>
            </div>
                  </li>
                  <li class="ability" data-ability="{{id}}">
                    <h4 class="ability-name box-title rollable">Agility</h4>
                    <select name="data.attackTypes">
                      {{#select data.data.initialRankValues }}
                      {{#each config.initialRankValues as |name type| }}
                      <option value="{{type}}">{{name}}</option>
                      {{/each}}
                      {{/select}}
                      </select>
                  </li>
                  <li class="ability" data-ability="{{id}}">
                    <h4 class="ability-name box-title rollable">Strength</h4>
                    <select name="data.attackTypes">
                      {{#select data.data.initialRankValues }}
                      {{#each config.initialRankValues as |name type| }}
                      <option value="{{type}}">{{name}}</option>
                      {{/each}}
                      {{/select}}
                      </select>
                  </li>
                  <li class="ability" data-ability="{{id}}">
                    <h4 class="ability-name box-title rollable">Endurance</h4>
                    <select name="data.attackTypes">
                      {{#select data.data.initialRankValues }}
                      {{#each config.initialRankValues as |name type| }}
                      <option value="{{type}}">{{name}}</option>
                      {{/each}}
                      {{/select}}
                      </select>
                  </li>
                  <li class="ability" data-ability="{{id}}">
                    <h4 class="ability-name box-title rollable">Reason</h4>
                    <select name="data.attackTypes">
                      {{#select data.data.initialRankValues }}
                      {{#each config.initialRankValues as |name type| }}
                      <option value="{{type}}">{{name}}</option>
                      {{/each}}
                      {{/select}}
                      </select>
                  </li>
                  <li class="ability" data-ability="{{id}}">
                    <h4 class="ability-name box-title rollable">Intuition</h4>
                    <select name="data.attackTypes">
                      {{#select data.data.initialRankValues }}
                      {{#each config.initialRankValues as |name type| }}
                      <option value="{{type}}">{{name}}</option>
                      {{/each}}
                      {{/select}}
                      </select>
                  </li>
                  <li class="ability" data-ability="{{id}}">
                    <h4 class="ability-name box-title rollable">Psyche</h4>
                    <select name="data.attackTypes">
                      {{#select data.initialRankValues }}
                      {{#each config.initialRankValues as |name type| }}
                      <option value="{{type}}">{{name}}</option>
                      {{/each}}
                      {{/select}}
                      </select>
                  </li>
              </ul>

              

        
  </div>
  {{!-- Biography Tab --}}
    <div class="tab biography" data-group="primary" data-tab="description">
      {{!-- If you want TinyMCE editors to output inline rolls when rendered, you need to pass the actor's roll data to the rollData property. --}}
    
    </div>

    {{!-- Owned Items Tab --}}
    <div class="tab items" data-group="primary" data-tab="items">
       {{> "systems/faserip/templates/actor/parts/actor-items.hbs"}}
    </div>

    {{!-- Owned Spells Tab --}}
    <div class="tab spells" data-group="primary" data-tab="spells">
      {{> "systems/faserip/templates/actor/parts/actor-spells.hbs"}}
    </div>

    {{!-- Active Effects Tab --}}
    <div class="tab effects flexcol" data-group="primary" data-tab="effects">
      {{> "systems/faserip/templates/actor/parts/actor-effects.hbs"}}
    </div>
</section>
</form>